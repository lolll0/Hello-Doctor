	<script type="text/javascript">
	function regist_go(){

		var title = document.getElementById("title").value;
		var content = document.getElementById("content").value;
		if(title == ""){
			blank_title();
			return false;
		}else if(content == ""){
			blank_content();
			return false;
		}else{
// 			success_regist();

			$("#regist").submit();

		}

	}
	function blank_title(){
		swal({
            title: "제목은 필수 사항입니다.",
            text: "",
            type: "warning"
        });
	}
	function blank_content(){
		swal({
            title: "내용은 필수 사항입니다.",
            text: "",
            type: "warning"
        });

	}
	function success_regist(){
		swal({
            title: "공지사항을 전송하시겠습니까?",
            text: "제목 : 약품업데이트",
            type: "warning",
            showCancelButton: true,
            confirmButtonColor: "#DD6B55",
            confirmButtonText: "확인",
            cancelButtonText: "취소",
            closeOnConfirm: false,
            closeOnCancel: false
        }, function (isConfirm) {
        	if(isConfirm){
        		swal("", "공지사항을 사용자에게 전송합니다.", "success");
                setTimeout(function(){
                	window.opener.location.reload();
                   window.close();
                },2000);

                form.submit();
        	}else{
        		swal("공지사항 전송을 취소합니다.");
        		 setTimeout(function(){
                     swal.close();
                  },1000);
        	}


        });
	}
	</script>
	    <script type="text/javascript">
    $(document).ready(function(){
        $('.i-checks').iCheck({
            checkboxClass: 'icheckbox_square-green',
            radioClass: 'iradio_square-green',
        });
    });
    </script>
    <script type="text/javascript">

	$(".pop").on("ifChecked", function(){
			$("#d").append(
					"<input id='popUpDate' style='margin-left:10px;' type='date'> ~ <input type='date'>");
			$("input[name=popYn]").attr("value", "Y");
	});
	$(".pop").on("ifUnchecked", function(){
		$("#d").empty();
		$("input[name=popYn]").attr("value", "N");
	})
    </script>
    <script type="text/javascript">
    $("input[name=pickYn]").on("ifChecked", function(){
    	$("input[name=pickYn]").attr("value", 'Y');
    });
    $("input[name=pickYn]").on("ifUnchecked", function(){
    	$("input[name=pickYn]").attr("value", 'N');
    });
    </script>

